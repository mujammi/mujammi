#include<stdio.h>
void main()
{
    int marks[5];
    int i;
    int sum=0;
    
    for(i=0;i<5;i++)
    {
        printf("Enter %d subject marks: ",i+1);
        scanf("%d",&marks[i]);
    }
    
    for(i=0;i<5;i++)
    {
        sum=sum+marks[i];
    }
    printf("Sum is :%d\n",sum);
    float avg=0;
    avg=sum/5;
    printf("Avg is :%f\n",avg);
    
    
}
